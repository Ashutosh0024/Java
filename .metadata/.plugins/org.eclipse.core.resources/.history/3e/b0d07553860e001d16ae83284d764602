package com.oops;

class Ebase{
	private String name;
	private int id;
	public Ebase(String name, int id) {
		super();
		this.name = name;
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	@Override
	public String toString() {
		return "name=" + name + ", id=" + id;
	}
	
	
}
public class Encapsulation {
	public static void main(String[] args) {
		Ebase obj = new Ebase("Ashutosh", 21);
		System.out.println(obj.toString());
		

	}

}
